<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.appmosphere">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon_leaf"
        android:label="@string/app_label"
        android:roundIcon="@mipmap/icon_leaf"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".NatureActivity"
            android:label="@string/nature_activity_label" />
        <activity
            android:name=".CityActivity"
            android:label="@string/city_activity_label" />
        <activity
            android:name=".FictionActivity"
            android:label="@string/fiction_activity_label" />
        <activity
            android:name=".CurrentSongActivity"
            android:label="@string/current_song_activity_label" />

    </application>

</manifest>